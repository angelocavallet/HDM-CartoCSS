        {
            "id": "poi.mss",
            "data": ".poi::mainprio[zoom>=14] {\n        [main='police'],\n        [main='cafe'],\n        [main='restaurant'],\n        [main='toilets'],\n        [main='shower'],\n        [main='fuel'],\n        [main='fast_food'],\n        [main='car_parts'],\n        [main='supermarket'],\n        [main='bakery'],\n        [main='car_repair'],\n        [main='fire_station'],\n        [main='shop'],\n        [main='doctors'],\n        [main='information'],\n        [main='hostel'],\n        [main='hotel'],\n        [main='motel'],\n        [main='clinic'],\n        [main='hospital'],\n        [main='tyres'] {\n            marker-file: url('icons/poi/[main].svg');\n            marker-clip:false;\n            marker-width: 12;\n        }\n}\n.poi::high[zoom>=17] {\n        [main='school'],\n        [main='aeroway'],\n        [main='bank'],\n        [main='government'],\n        [main='pharmacy'],\n        [main='townhall'],\n        [main='taxi'],\n        [main='car_rental'],\n        [main='money_transfer'],\n        [main='dentist'],\n        [main='bureau_de_change'],\n        [main='telephone'],\n        [main='university'],\n        [main='mobile_phone'],\n        [main='tailor'],\n        [main='computer'],\n        [main='clothes'],\n        [main='butcher'],\n        [main='hardware'],\n        [main='atm'],\n        [main='post_office'],\n        [main='government'] {\n            marker-file: url('icons/poi/[main].svg');\n            marker-clip:false;\n        }\n}\n"
